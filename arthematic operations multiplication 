1.
a = 7
b = 6
print(a * b)


2.
num1 = int(input("Enter first number: "))
num2 = int(input("Enter second number: "))
result = num1 * num2
print(result)


3. numbers = [2, 3, 4]
product = 1

for num in numbers:
    product *= num

print(product)


4. 
num = 5

for i in range(1, 11):
    print(num * i)


5. 
for i in range(1, 4):
    for j in range(1, 4):
        print(i * j, end=" ")
    print()

