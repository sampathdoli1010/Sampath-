1. 
a = 15
b = 7
result = a - b
print(result)

2. 
num1 = int(input("Enter first number: "))
num2 = int(input("Enter second number: "))
print(num1 - num2)

3. 
x = 10.5
y = 4.2
print(x - y)

4. a = 50
b = 20
c = 5
total = a - b - c
print(total)

5. 
numbers = [30, 5, 10]
result = numbers[0]

for num in numbers[1:]:
    result -= num

print(result)